{
  "name": "@cloudpower97/react-advanced-style-proptypes",
  "version": "0.0.1",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "dev": "build:watch",
    "test:coverage": "cross-env BABEL_ENV=test jest --env=jsdom --coverage && cat ./coverage/lcov.info | codacy-coverage && rm -rf ./coverage",
    "test:unit": "cross-env BABEL_ENV=test jest --env=jsdom",
    "test:unit:watch": "cross-env BABEL_ENV=test jest --env=jsdom --watch",
    "prebuild": "npm-run-all clean",
    "build": "cross-env BABEL_ENV=production babel -d lib src",
    "build:watch": "cross-env NODE_ENV=development babel -d lib src --watch",
    "lint": "eslint src/**/*.js",
    "fix": "npm-run-all lint -- --fix",
    "pretty": "prettier --write src/**/*.js",
    "release": "run-s build release-it"
  },
  "files": [
    "lib",
    "src"
  ],
  "homepage": "https://github.com/CloudPower97/react-advanced-style-proptypes",
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudPower97/react-advanced-style-proptypes.git"
  },
  "keywords": [
    "react",
    "proptype",
    "validation",
    "devtool",
    "style",
    "css"
  ],
  "author": {
    "name": "Claudio Cortese",
    "url": "https://github.com/CloudPower97"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CloudPower97/react-advanced-style-proptypes/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn pretty",
      "yarn fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-jsdoc-to-assert": "^4.0.0",
    "babel-preset-minify": "^0.5.0",
    "codacy-coverage": "^3.3.0",
    "conventional-changelog-cli": "^2.0.11",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "regenerator-runtime": "^0.13.1",
    "release-it": "^8.2.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "known-css-properties": "^0.11.0",
    "to-slug-case": "^1.0.0"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1",
      "^properties(.*)$": "<rootDir>/src/properties$1"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.jsx?$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
